{
  "extends": [
    "eslint:recommended"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "parser": "@typescript-eslint/parser",
  "env": {
    "es6": true,
    "node": true,
    "commonjs": true
  },
  "parserOptions": {
    "sourceType": "module",
    "project": "./tscondig.json"
  },
  "rules": {
    "no-unused-vars": "error",
    "no-console": [
      "warn", 
      {
        "allow": ["warn", "error", "info"]
      }
    ]
  }
}